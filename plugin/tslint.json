{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],    "indent": [
            true,
            "spaces",
            2
        ],
        "max-line-length": [
            true,
            150
        ],
        "interface-name": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-consecutive-blank-lines": false,
        "no-string-literal": false
    },
    "rulesDirectory": []
}
